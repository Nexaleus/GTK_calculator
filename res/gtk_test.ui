<interface>
  <!-- interface-name gtk_test.ui -->
  <requires lib="gtk" version="4.18"/>
  <object class="GtkApplication" id="app.xorrcxrcx.calculator"/>
  <object class="GtkApplicationWindow">
    <property name="application">app.xorrcxrcx.calculator</property>
    <property name="default-height">480</property>
    <property name="default-width">640</property>
    <property name="title">Calculator</property>
    <property name="visible">True</property>
    <child>
      <object class="GtkGrid">
        <child>
          <object class="GtkButton" id="btn_showtest">
            <property name="halign">center</property>
            <property name="has-tooltip">True</property>
            <property name="label">CLICK ME</property>
            <property name="tooltip-text">This is a tooltip for a button</property>
            <property name="valign">center</property>
            <signal name="clicked" handler="btn_clicked" object="label_showtest" swapped="no"/>
            <layout>
              <property name="column">2</property>
              <property name="column-span">1</property>
              <property name="row">2</property>
              <property name="row-span">1</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="GtkSwitch">
            <property name="halign">center</property>
            <property name="valign">end</property>
            <layout>
              <property name="column">2</property>
              <property name="column-span">1</property>
              <property name="row">1</property>
              <property name="row-span">1</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="GtkLabel" id="label_showtest">
            <property name="halign">center</property>
            <property name="label">THE BUTTON WAS CLICKED</property>
            <property name="selectable">True</property>
            <property name="valign">center</property>
            <property name="visible">False</property>
            <layout>
              <property name="column">2</property>
              <property name="column-span">1</property>
              <property name="row">3</property>
              <property name="row-span">1</property>
            </layout>
          </object>
        </child>
      </object>
    </child>
  </object>
</interface>
