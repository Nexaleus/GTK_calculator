{
    "version": 2,
    "configurePresets": [
      {
        "name": "win_x64_debug_vcpkg",
        "displayName": "App Win64 Debug VCPKG_CONFIG",
        "generator": "Ninja",
        "binaryDir": "${sourceDir}/build/debug",
        "cacheVariables": {
          "CMAKE_TOOLCHAIN_FILE": {
          "value": "$env{VCPKG_ROOT}/scripts/buildsystems/vcpkg.cmake",
          "type": "FILEPATH"
          }, "CMAKE_BUILD_TYPE": "Debug",
          "CMAKE_C_COMPILER": "cl"
        },
        "toolset": {
          "strategy": "external"
        },
        "architecture":{
          "strategy": "external",
          "value": "x64"
        }
      },
      {
        "name": "win_x64_release_vcpkg",
        "displayName": "App Win64 Release VCPKG_CONFIG",
        "generator": "Ninja",
        "binaryDir": "${sourceDir}/build/release",
        "cacheVariables": {
          "CMAKE_TOOLCHAIN_FILE": {
          "value": "$env{VCPKG_ROOT}/scripts/buildsystems/vcpkg.cmake",
          "type": "FILEPATH"
          }, "CMAKE_BUILD_TYPE": "Release",
          "CMAKE_C_COMPILER": "cl"
        },
        "toolset": {
          "strategy": "external"
        },
        "architecture":{
          "strategy": "external",
          "value": "x64"
        }
      }
    ],"buildPresets": [
      {
        "name": "app_win_x64_debug",
        "displayName": "App Win64 Debug BUILD",
        "configurePreset": "win_x64_debug_vcpkg",
        "configuration": "Debug"
      },
       {
        "name": "app_win_x64_release",
        "displayName": "App Win64 Release BUILD",
        "configurePreset": "win_x64_release_vcpkg",
        "configuration": "Release"
      }
    ]
  }